{
  "manifest_version": 3,
  "name": "Print file",
  "version": "1.0.0",
  "description": "A pdf & fpx files print Chrome extension with React.",
  "permissions": ["webRequest", "activeTab", "storage", "tabs", "downloads", "printerProvider", "scripting", "nativeMessaging"],
  "background": {
    "service_worker": "/src/pages/background/index.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/src/pages/content/index.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/src/pages/background/index.js", "/src/pages/content/index.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "/src/pages/popup/index.html",
    "default_title": "Click to show an alert"
  },
  "commands": {
    "print-file": {
      "suggested_key": {
        "default":  "Ctrl+Shift+P"
      },
      "description": "Print this file"
    }
  },
  "icons": {
    "34": "icon-34.png",
    "128": "icon-128.png"
  }
}
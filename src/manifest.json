{
  "manifest_version": 3,
  "name": "Print file",
  "version": "1.0.0",
  "description": "A pdf & fpx files print Chrome extension with React.",
  "action": {
    "default_popup": "printing/printers.html"
  },
  "permissions": ["storage", "userScripts", "printing"],
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "content/contentScript.js"
        ]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}